ARG GOBEL_MYSQL_EXPORTER_IMAGE_NAME
ENV GOBEL_MYSQL_EXPORTER_IMAGE_NAME=${GOBEL_MYSQL_EXPORTER_IMAGE_NAME}

ARG GOBEL_MYSQL_EXPORTER_IMAGE_TAG
ENV GOBEL_MYSQL_EXPORTER_IMAGE_TAG=${GOBEL_MYSQL_EXPORTER_IMAGE_TAG}

FROM --platform=linux/amd64 ${GOBEL_MYSQL_EXPORTER_IMAGE_NAME}}:${GOBEL_MYSQL_EXPORTER_IMAGE_TAG}}