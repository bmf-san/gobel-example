ARG GOBEL_NODE_EXPORTER_IMAGE_NAME
ENV GOBEL_NODE_EXPORTER_IMAGE_NAME=${GOBEL_NODE_EXPORTER_IMAGE_NAME}

ARG GOBEL_NODE_EXPORTER_IMAGE_TAG
ENV GOBEL_NODE_EXPORTER_IMAGE_TAG=${GOBEL_NODE_EXPORTER_IMAGE_TAG}

FROM --platform=linux/amd64 ${GOBEL_NODE_EXPORTER_IMAGE_NAME}}:${GOBEL_NODE_EXPORTER_IMAGE_TAG}}